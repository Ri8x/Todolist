{
  "name": "todolist",
  "version": "0.53.17",
  "author": {
    "name": "blaadje",
    "email": "acharlot91@gmail.com"
  },
  "description": "Be productive quickly",
  "license": "MIT",
  "main": "dist/electron/main.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently -p \"{name}\" -n \"ðŸš‹ Webpack,ðŸ”Œ Electron\" -c \"green.bold,yellow.bold\" \"webpack-dev-server --config=./config/dev/webpack.config.js --mode development\" \"sleep 2 && electron ./src/main.js\"",
    "build": "node config/prod/build.js"
  },
  "engines": {
    "node": "~12.14.x"
  },
  "build": {
    "productName": "Todolist",
    "appId": "io.blaadje.todo-list",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "icons/icon.icns"
    },
    "win": {
      "icon": "icons/icon.ico"
    },
    "linux": {
      "icon": "icons"
    }
  },
  "dependencies": {
    "lowdb": "^1.0.0",
    "moment": "^2.18.1",
    "moment-range": "^3.0.3",
    "vue": "^2.3.3",
    "vue-color": "^2.3.2",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^8.0.6",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "concurrently": "^5.0.2",
    "css-loader": "^0.28.4",
    "del": "^2.2.1",
    "electron": "^7.1.9",
    "electron-builder": "^22.2.0",
    "electron-debug": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^10.2.1",
    "html-webpack-plugin": "^3.2.0",
    "multispinner": "^0.2.1",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.1",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^0.18.1",
    "svg-to-vue-component": "^0.3.8",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
